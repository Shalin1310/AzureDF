{
	"name": "blob_to_sql",
	"properties": {
		"activities": [
			{
				"name": "copy_blob_to_sql_csv",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "truncate table [SSIS_DEMO].[LZ].[Fact];",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_blob_fact",
						"type": "DatasetReference",
						"parameters": {
							"filename_DS": {
								"value": "@pipeline().parameters.filename",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Local_SqlServerTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"filename": {
				"type": "string",
				"defaultValue": "abc.csv"
			}
		},
		"folder": {
			"name": "Testing 1"
		},
		"annotations": [],
		"lastPublishTime": "2021-02-25T13:19:28Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}